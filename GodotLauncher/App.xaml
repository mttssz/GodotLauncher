<Application x:Class="GodotLauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GodotLauncher"
             xmlns:System="clr-namespace:System;assembly=mscorlib"
             DispatcherUnhandledException="App_OnDispatcherUnhandledException"
             Startup="App_OnStartup"
             Exit="App_OnExit">
    <Application.Resources>
        <System:String x:Key="VersionsFileUrl">https://raw.githubusercontent.com/mttssz/GodotLauncher/master/versions.json</System:String>
        <System:String x:Key="GodotLauncherVersion">1.0.0</System:String>
    </Application.Resources>
</Application>
