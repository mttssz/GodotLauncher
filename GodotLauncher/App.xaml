<Application x:Class="GodotLauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GodotLauncher"
             xmlns:clr="clr-namespace:System;assembly=mscorlib"
             DispatcherUnhandledException="App_OnDispatcherUnhandledException"
             Startup="App_OnStartup"
             Exit="App_OnExit">
    <Application.Resources>
        <ResourceDictionary>
            <clr:String x:Key="EnvironmentFileName">GodotLauncherEnvironment.json</clr:String>
            <clr:String x:Key="SourceUrlBase">https://downloads.tuxfamily.org/godotengine/</clr:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>
